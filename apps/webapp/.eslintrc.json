{
  "extends": ["../../.eslintrc.json", "next/core-web-vitals"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".jsx", ".tsx"] }],
    "react/function-component-definition":[2, { "namedComponents": "arrow-function" }],
    "react/jsx-props-no-spreading": "off"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "module":false
  }
}
